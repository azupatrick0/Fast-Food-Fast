{
  "name": "fast-food-fast",
  "version": "1.0.0",
  "description": "Fast-Food-Fastâ€‹ is a food delivery service app for a restaurant.",
  "scripts": {
    "start": "npm run start:webpack:prod && npm run build && node ./Server/dist/index.js ",
    "start:dev": "npm run start:webpack:dev & npm run create-table-users-dev && npm run create-table-items-dev && npm run create-table-orders-dev && cross-env NODE_ENV=development nodemon --exec babel-node ./Server/src/index.js",
    "start:webpack:dev": "webpack --mode development --watch",
    "start:webpack:prod": "webpack --mode production",
    "pretest": "npm run lint",
    "test": "npm run create-table-users-test && npm run create-table-items-test && npm run create-table-orders-test && cross-env NODE_ENV=test nyc mocha --exit  ./Server/src/test/testSpec.js --require ./Client/setup.js ./Client/test/index.js --require @babel/register --require ignore-styles --timeout 250000",
    "test:client": "cross-env NODE_ENV=test nyc mocha --exit --require ./Client/setup.js ./Client/test/index.js --require @babel/register --require ignore-styles --timeout 250000",
    "test:e2e": "testcafe chrome ./Client/test/e2e/order.js",
    "build": "npm run clean && babel ./Server/src -d ./Server/dist",
    "lint": "./node_modules/.bin/eslint ./Client ./Server --fix --ignore-path .gitignore",
    "clean": "rimraf ./Server/dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "create-table-users-dev": "cross-env NODE_ENV=development babel-node ./Server/src/models/users.js",
    "create-table-items-dev": "cross-env NODE_ENV=development babel-node ./Server/src/models/menu.js",
    "create-table-orders-dev": "cross-env NODE_ENV=development babel-node ./Server/src/models/orders.js",
    "create-table-users-test": "cross-env NODE_ENV=test babel-node ./Server/src/models/users.js",
    "create-table-items-test": "cross-env NODE_ENV=test babel-node ./Server/src/models/menu.js",
    "create-table-orders-test": "cross-env NODE_ENV=test babel-node ./Server/src/models/orders.js"
  },
  "engines": {
    "node": "10.16.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azupatrick0/Fast-Food-Fast.git"
  },
